<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mkdesignsoftware.testingtortuga" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".StartActivity"
            android:label="@string/title_activity_start" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.mkdesignsoftware.testingtortuga.MainActivity"

                android:parentActivityName=".MainActivity"/>
        </activity>
        <activity
            android:name=".Settings_and_Contact_Activity"
            android:label="@string/title_activity_settings_and__contact_" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.mkdesignsoftware.testingtortuga.MainActivity"
                android:parentActivityName=".MainActivity" />
        </activity>
    </application>

</manifest>
